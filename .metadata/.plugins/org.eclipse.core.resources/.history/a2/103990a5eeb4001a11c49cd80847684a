package classpart;

public class Student { // 클래스 이름 Student

	// 객체속성, 멤버변수 
	int studentId;		  // 학번
	String studentName;  // 학생이름
	int grade; 			// 학년
	String address;	   // 사는곳
	
	//학생 이름과 주소 출력하는 메서드 만들기 
	public void showStudentInfo() {
		System.out.println(studentName +" , "+ address);
	}
}



