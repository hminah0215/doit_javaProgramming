package loopexample;

public class Test4 {
	public static void main(String[] args) {
//		*
//		***
//		*****
//		*******
		
		for(int i = 0; i <= 7 ; i++) {
			if(i%2 != 0)
				continue;
			switch (i) {
			case 3:
				System.out.println("***");
				break;

			case 5:
				System.out.println("*****");
				break;
				
			case 7:
				System.out.println("*******");
				break;
			}
		}
	}
}
